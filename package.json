    "@typescript-eslint/parser": "^5.20.0",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^8.14.0",
    "eslint-plugin-github": "^4.2.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.0.6",
    "jest-circus": "^27.0.6",
    "js-yaml": "^4.1.0",
    "prettier": "^2.3.2",
